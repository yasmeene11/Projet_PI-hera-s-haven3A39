<style>
    .rating-stars {
        display: flex;
        cursor: pointer;
    }

    .star {
        margin-right: 5px;
        font-size: 24px;
        transition: transform 0.2s ease-in-out;
    }

    .star:hover {
        transform: scale(1.2); /* Increase the size on hover */
    }
</style>
<script>
    document.addEventListener('DOMContentLoaded', function () {
        const stars = document.querySelectorAll('.rating-stars .star');
        const csrfToken = document.querySelector('meta[name="csrf-token"]').content;

        stars.forEach(star => {
            star.addEventListener('click', function () {
                const productId = this.closest('.rating-stars').dataset.productId;
                const rating = this.dataset.rating;

                // Send a request to your Symfony controller to handle the rating update
                fetch(`/update-rating/${productId}/${rating}`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'X-CSRF-Token': csrfToken, // Include CSRF token in headers
                    },
                    credentials: 'same-origin',  // Include credentials for same-origin requests
                })
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
                })
                .then(data => {
                    // Update the UI or handle success response
                    console.log('Rating updated successfully:', data);
                })
                .catch(error => {
                    console.error('Error updating rating:', error);
                });
            });
        });
    });
</script>


