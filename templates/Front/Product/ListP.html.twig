{% block content %}
    <div class="sidebar-search">
        <div class="input-group custom-search-form">
            <input type="text" id="search" class="form-control" placeholder="Search here">
        </div>
    </div>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Quantity</th>
                <th>Label</th>
                <th>Expiration Date</th>
                <th>Category</th>
                <th>Image</th>
            </tr>
        </thead>
        <tbody id="t">
            {# Your products will be dynamically inserted here through JavaScript #}
        </tbody>
    </table>

    <script>
        document.addEventListener('DOMContentLoaded', function() {
            var searchInput = document.getElementById('search');
            var tbody = document.getElementById('t');

            searchInput.addEventListener('keyup', function() {
                var value = searchInput.value;
                var xhr = new XMLHttpRequest();

                xhr.open('GET', "{{ path('search_result') }}?searchValue=" + encodeURIComponent(value), true);
                xhr.onreadystatechange = function() {
                    if (xhr.readyState === 4 && xhr.status === 200) {
                        var response = JSON.parse(xhr.responseText);
                        updateTable(response);
                    }
                };

                xhr.send();
            });

            function updateTable(products) {
                tbody.innerHTML = '';

                if (products.length > 0) {
                    products.forEach(function(product) {
                        tbody.innerHTML += '<tr><td>' + product.productId + '</td><td>' + product.Product_Name + '</td>' +
                            '<td>' + product.Product_Quantity + '</td><td>' + product.Product_Label + '</td>' +
                            '<td>' + (product.Expiration_Date ? product.Expiration_Date : '') + '</td>' +
                            '<td>' + (product.Category_Key ? product.Category_Key.Product_Type : '') + '</td>' +
                            '<td>' + product.Product_Image + '</td>' ;
                    });
                } else {
                    tbody.innerHTML = '<tr><td colspan="8">no records found</td></tr>';
                }
            }
        });
    </script>
