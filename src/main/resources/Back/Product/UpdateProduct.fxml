<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="410.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Back.Product.UpdateProductB">

    <Label text="Product Name:" />
    <TextField fx:id="txtProductName" />

    <Label text="Product Label:" />

    <TextField fx:id="txtProductLabel" />
    <Label text="Product Quantity:" />

    <TextField fx:id="txtProductQuantity" />
    <Label text="Expiration Date:" />

    <DatePicker fx:id="txtExpirationDate" />
    <Label text="Category:" />

    <ComboBox fx:id="txtCategory" prefHeight="25.0" prefWidth="178.0" />

    <Button onAction="#updateProduct" text="Update" />
</VBox>
